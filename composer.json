{
  "name"       : "matthewbaggett/uuid",
  "description": "A nice little library to interact with UUIDs",
  "authors"    : [
    {
      "name" : "Matthew Baggett",
      "email": "matthew@baggett.me"
    }
  ],
  "replace": {
    "gone.io/uuid": "*"
  },
  "config": {
    "sort-packages": true
  },
  "license": "GPL-3.0-or-later",
  "require"    : {
    "php": "^7.4 || ^8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "kint-php/kint": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-phpunit": "^0.12.16",
    "phpstan/phpstan-symfony": "^0.12",
    "phpunit/phpcov": "*",
    "phpunit/phpunit": "^9",
    "slam/phpstan-extensions": "^5",
    "thecodingmachine/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {"MatthewBaggett\\UUID\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "MatthewBaggett\\UUID\\Tests\\": "tests/" }
  }
}
